# Database Configuration
DATABASE_URL=
DATABASE_DIALECT=postgres
PGHOST=<db host>
PGPORT=5432
PGUSER=<db username>
PGPASSWORD=<db password>
PGDATABASE=<db name>

# App config
NODE_ENV=<enviroment>
PORT=<PORT>
WHITELISTED_ORIGINS=<origins>

# JWT
JWT_SECRET_KEY=<jwt secret key>
JWT_EXPIRES_IN=<jwt expiry>

# Python Backend Configuration
PYTHON_BACKEND_URL=<python backend url>
PYTHON_BACKEND_API_KEY=<your-python-backend-api-key>
PYTHON_BACKEND_TIMEOUT=<python-backend-timeout>
PYTHON_BACKEND_RETRY_ATTEMPTS=3

# File Upload Configuration
MAX_FILE_SIZE=<file size>
UPLOAD_PATH=./uploads

# Logging Configuration
LOG_LEVEL=debug
LOG_FILE=./logs/app.log

# Swagger Configuration
SWAGGER_TITLE=User & Document Management API
SWAGGER_DESCRIPTION=NestJS backend for user authentication, document management, and ingestion controls
SWAGGER_VERSION=1.0.0
SWAGGER_PATH=api/docs